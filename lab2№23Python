x1,y1,r1 = map(int, input("Введите координаты и радиус первой окружности ").split())
x2,y2,r2 = map(int, input("Введите координаты и радиус второй окружности ").split())
rs =((x1 - x2) ** 2 + (y1 - y2) ** 2)
if rs == (r1 + r2)**2:
    print("Окружности касаются")
else:
    print("Окружности НЕ касаются")
